package test;

import java.io.File;

public class Data {
	
	String bookingName,flightNumber, seatCategory,paymentCardNumber, numberOfSeats;
	File outputCSV, outputTxt;

	public Data(String bookingName, String flightNumber, String seatCategory, String numberOfSeats, String paymentCardNumber, File outputCSV, File outputTxt) {
		this.bookingName = bookingName;
		this.flightNumber = flightNumber;
		this.seatCategory = seatCategory;
		this.numberOfSeats = numberOfSeats;
		this.paymentCardNumber = paymentCardNumber;
		this.outputCSV = outputCSV;
		this.outputTxt = outputTxt;
	}

}
